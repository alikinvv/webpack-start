{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jquery": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "extends": "standard",
    "rules": {
        "no-undef": 0,
        "indent": ["error", 4],
        "semi": [2, "always"]
    }
}